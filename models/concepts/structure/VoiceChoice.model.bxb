structure (VoiceChoice) {
	description (This structure gets returned to the user to select a voice)
	property (voice) {
		type (Voice)
		min (Required) max (One)
	}
	property (language) {
		type (viv.core.Text)
		min (Required) max (One)
		visibility (Private)
	}
	property (image) {
		type (viv.core.Text)
		min (Optional) max (One)
		visibility (Private)
	}
}
