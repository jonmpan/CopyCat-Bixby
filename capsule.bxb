capsule {
	store-countries{
		all
	}
	id (playground.copycat)
	version (0.0.14)
	format (3)
	targets {
		target (bixby-mobile-en-US)
	}
	runtime-flags {
		allow-dialogs-on-detail-pages
		modern-default-view-behavior
